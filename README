For assignment
#major - all years
par(mfrow = c(2,2))
denscomp(list(major.freq.fit.exp, major.freq.fit.nb), legendtext = c("exponential", "negative binomial"))
cdfcomp(list(major.freq.fit.exp, major.freq.fit.nb), legendtext = c("exponential", "negative binomial"))
qqcomp(list(major.freq.fit.exp, major.freq.fit.nb), legendtext = c("exponential", "negative binomial"))
ppcomp(list(major.freq.fit.exp, major.freq.fit.nb), legendtext = c("exponential", "negative binomial"))

#major - post 2005 data
par(mfrow = c(2,2))
denscomp(list(major.freq.fit.exp.1, major.freq.fit.nb.1), legendtext = c("exponential", "negative binomial"))
cdfcomp(list(major.freq.fit.exp.1, major.freq.fit.nb.1), legendtext = c("exponential", "negative binomial"))
qqcomp(list(major.freq.fit.exp.1, major.freq.fit.nb.1), legendtext = c("exponential", "negative binomial"))
ppcomp(list(major.freq.fit.exp.1, major.freq.fit.nb.1), legendtext = c("exponential", "negative binomial"))

